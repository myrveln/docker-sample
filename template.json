{
    "family": "kimtestar",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "kimtestar",
            "image": "public.ecr.aws/nginx/nginx:alpine-slim",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "NAME2",
                    "value": "VALUE2"
                },
                {
                    "name": "NAME1",
                    "value": "VALUE1"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/kimtestar",
                    "awslogs-region": "eu-north-1",
                    "awslogs-stream-prefix": "kimtestar"
                }
            }
        }
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "ARM64",
        "operatingSystemFamily": "LINUX"
    }
}
